{
  "site": {
    "title": "PxG Profit",
    "base_url": "https://astroplate.netlify.app",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/images/favicon.png",
    "logo": "/images/logo.png",
    "logo_darkmode": "/images/logo-darkmode.png",
    "logo_width": "150",
    "logo_height": "30",
    "logo_text": "PxG Profit"
  },
  "settings": {
    "search": true,
    "sticky_header": true,
    "theme_switcher": true,
    "default_theme": "system",
    "pagination": 2,
    "summary_length": 200,
    "blog_folder": "blog"
  },
  "params": {
    "contact_form_action": "#",
    "copyright": "Developed by Jviper - Theme [Astroplate](https://github.com/zeon-studio/astroplate)"
  },
  "navigation_button": {
    "enable": true,
    "label": "Sobre",
    "link": "/about"
  },
  "disqus": {
    "enable": true,
    "shortname": "themefisher-template",
    "settings": {}
  },
  "metadata": {
    "meta_author": "jviper",
    "meta_image": "/images/og-image.png",
    "meta_description": "astro and tailwind boilerplate"
  }
}